        "express": "~4.16.1",
        "express-redis-cache": "^1.1.3",
        "express-winston": "^4.0.1",
        "fetch": "^1.1.0",
        "http-errors": "~1.6.3",
        "morgan": "^1.9.1",
        "node": "^13.1.0",
        "node-fetch": "^2.6.0",
        "pug": "^2.0.4",
        "redis": "^2.8.0",
        "redis-rejson": "^1.0.0",
        "winston": "^3.2.1"
    },
    "devDependencies": {
        "@babel/core": "^7.7.5",
        "@babel/node": "^7.7.4",
        "@babel/preset-env": "^7.7.6",
        "babel-loader": "^8.0.6",
        "eslint": "^6.7.2",
        "eslint-config-airbnb-base": "^14.0.0",
        "eslint-config-prettier": "^6.7.0",
